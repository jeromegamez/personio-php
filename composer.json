{
    "name": "gamez/personio",
    "description": "Interact with Personio (https://www.personio.de) from your PHP application.",
    "keywords": ["personio", "api", "sdk", "time tracking"],
    "license": "MIT",
    "homepage": "https://github.com/jeromegamez/personio-php",
    "authors": [
        {
            "name": "Jérôme Gamez",
            "homepage": "https://github.com/jeromegamez"
        }
    ],
    "require": {
        "php": ">=7.2",
        "ext-mbstring": "*",
        "psr/http-client": "^1.0",
        "psr/http-factory": "^1.0",
        "psr/http-message": "^1.0"
    },
    "require-dev": {
        "guzzlehttp/guzzle": "^6.3|^7.3",
        "kriswallsmith/buzz": "^1.2",
        "nyholm/psr7": "^1.0",
        "symfony/var-dumper": "^4.2|^5.2"
    },
    "suggest": {
        "guzzlehttp/guzzle": "Allows the usage of the Guzzle Api Client",
        "psr/http-client-implementation": "Allows the usage of the PSR-18 Api Client",
        "psr/http-factory-implementation": "Required for the PSR-18 Api Client"
    },
    "autoload": {
        "psr-4": {
            "Gamez\\Personio\\": "src"
        }
    },
    "config": {
        "sort-packages": true
    }
}
